  <project path="device/xiaomi/dipper" remote="OuvrirOS-Devices" name="android_device_xiaomi_dipper" revision="main" groups="device,dipper" />
  <project path="device/qcom/qssi" remote="OuvrirOS" name="android_device_qcom_qssi" revision="sdm845" groups="device,qcom_sdm845,qssi" />

  <!-- Kernel -->
  <project path="kernel/xiaomi/dipper" remote="OuvrirOS" name="android_kernel_xiaomi_dipper" revision="main" groups="pdk" />

  <!-- Vendor -->

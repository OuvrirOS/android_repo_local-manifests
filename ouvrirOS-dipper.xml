<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="Octavi-OS" fetch="https://github.com/Octavi-OS/" />
  <remote name="OuvrirOS" fetch="https://github.com/OuvrirOS/" />
  <remote name="OuvrirOS-Devices" fetch="https://github.com/OuvrirOS-Devices/" />

  <!-- Device Trees -->

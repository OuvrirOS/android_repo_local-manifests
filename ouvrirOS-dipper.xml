  <project path="vendor/qcom/opensource/data-ipa-cfg-mgr" remote="OuvrirOS" name="android_vendor_qcom_opensource_data-ipa-cfg-mgr" revision="main" groups="generic_fs,vendor" />
  <project path="vendor/qcom/opensource/dataservices" remote="OuvrirOS" name="android_vendor_qcom_opensource_dataservices" revision="main" groups="generic_fs,vendor" />
  <project path="vendor/qcom/opensource/display" remote="OuvrirOS" name="android_vendor_qcom_opensource_display" revision="main" groups="generic_fs,vendor" />
  <project path="vendor/qcom/opensource/usb" remote="OuvrirOS" name="android_vendor_qcom_opensource_usb" revision="main" groups="generic_fs,vendor" />

  <project path="vendor/nxp/opensource/pn5xx" remote="OuvrirOS" name="android_vendor_nxp_opensource_pn5xx_halimp" revision="main" groups="generic_fs,vendor" />

  <project path="vendor/ouvrir" remote="OuvrirOS-Devices" name="android_vendor_ouvrir" revision="main" groups="generic_fs,vendor,ouvrir" />
  <project path="vendor/xiaomi/dipper" remote="OuvrirOS-Devices" name="proprietary_vendor_xiaomi_dipper" revision="main" groups="generic_fs,vendor,dipper" />
  <project path="vendor/xiaomi/sdm845-common" remote="OuvrirOS-Devices" name="proprietary_vendor_xiaomi_sdm845-common" revision="main" groups="generic_fs,vendor,qcom_sdm845" />
